<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.musicalapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ArtistasActivity" android:label="@string/categoria_artistas"></activity>
        <activity android:name=".AlbunsActivity" android:label="@string/categoria_albuns" />
        <activity android:name=".DowloadsActivity" android:label="@string/categoria_dowloads" />
        <activity android:name=".FavoritasActivity" android:label="@string/categoria_favoritas" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>